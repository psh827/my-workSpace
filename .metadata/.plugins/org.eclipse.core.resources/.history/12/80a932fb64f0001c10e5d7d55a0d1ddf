package com.varxyz.banking.domain;

public class CustomerServiceTest {
	public static void main(String[] args) {
		CustomerService2 customer = new CustomerServiceImpl2();
		Customer park = new Customer();
		park.setName("pakr");
		park.setSsn("960827-123456");
		park.setPasswd("tkdgns");
		Account parkAcc = new Account();
		parkAcc.setAccountNum("3333-09-6233");
		parkAcc.setBalance(7.5);
		park.setAccount(parkAcc);
		customer.addCustomer(park);
		System.out.println(customer.getCustomerBySsn("960827-123456"));
		System.out.println(customer.getAllCustomers());
		System.out.println(customer.getNumOfCustomers());
		
	}
	
}
