package com.varxyz.banking.domain;

import java.util.*;

public class Customer extends Account{
	private String name;
	private String ssn;
	private String phone;
	private String customerId;
	private String passwd;
	List<Account> account = new ArrayList<Account>();
	
	public Customer() {
		// TODO Auto-generated constructor stub
	}
	
	public Customer(String name, String ssn, String phone, String customerId, String passwd, Account account) {
		this.name = name;
		this.ssn = ssn;
		this.phone = phone;
		this.customerId = customerId;
		this.passwd = passwd;
		this.account = (List<Account>) account;
	}
	

}
